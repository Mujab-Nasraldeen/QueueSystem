@model List<PatientAppointmentRespDto>

@{
    ViewData["Title"] = "CallingPatientAgain";
}

<h2>  <i class="bi bi-clipboard2-check"></i> Calling Patients Again </h2>

<div class="mt-4">
    @if (!Model.Any())
    {
        <div class="alert alert-warning" role="alert">
            <i class="bi bi-exclamation-triangle"></i>
            No Patients found!
        </div>
    }
    else
    {
        <ul>
            @foreach(var Appointment in Model)
            {
                <li>@Appointment.Id | @Appointment.UserName | @Appointment.FullName | @Appointment.Gender</li>
            }
        </ul>
    }
</div>